{
  "name": "@indutny/frameless-titlebar",
  "version": "2.3.4",
  "description": "Customizable titlebar for frameless Electron desktop applications",
  "author": "Cristian006",
  "license": "MIT",
  "repository": "Cristian006/frameless-titlebar",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom --passWithNoTests",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@rollup/plugin-strip": "^1.3.2",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.22",
    "cross-env": "^5.2.0",
    "gh-pages": "^2.0.1",
    "react": "^17.0.0",
    "react-scripts": "^3.4.0",
    "react-test-renderer": "^17.0.0",
    "rollup": "2.75.5",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-typescript2": "0.32.0",
    "rollup-plugin-url": "^2.1.0",
    "typescript": "4.7.3"
  }
}
